<!doctype html>
<html>
<head>
    <link rel="stylesheet" type="text/css" href="/webjars/bootstrap/4.4.1-1/css/bootstrap.min.css"/>
    <link rel="stylesheet" type="text/css" href="/css/main.css">
    <link rel="stylesheet" type="text/css" href="/webjars/datatables/1.10.20/css/dataTables.bootstrap.min.css">
    {{#block "head"}}
    {{!-- Custom <head> content per page could be added. --}}
    {{/block}}
</head>
<body>
<header>
    {{#block "bodyHeader"}}
    {{!-- Hero content goes here. --}}
    {{/block}}
</header>

<div class="container">
        <!-- edit form column -->
        <form class="form-horizontal" role="form">
            <div class="form-group">
                <label class="col-lg-3 control-label">User Name</label>
                <div class="col-lg-8">
                    <input class="form-control" type="text" id="userName" name="userName" value="" readonly>
                </div>
            </div>
            <div class="form-group">
                <label class="col-lg-3 control-label">Account Number</label>
                <div class="col-lg-8">
                    <input class="form-control" type="text" id="accountNumber" name="accountNumber" value="" readonly>
                </div>
            </div>
            <div class="form-group">
                <label class="col-lg-3 control-label">Account Type</label>
                <div class="col-lg-8">
                    <input class="form-control" type="text" id="accountType" name="accountType" value="" readonly>
                </div>
            </div>
            <div class="form-group">
                <label class="col-lg-3 control-label">Account Balance</label>
                <div class="col-lg-8">
                    <input class="form-control" type="text" id="accountBalance" name="accountBalance" value="" readonly>
                </div>
            </div>
            <div class="form-group">
                <label class="col-lg-3 control-label">Last five Transactions</label>
                <div class="col-lg-8">
                    <input class="form-control" type="text" id="lastFiveTransactions" name="lastFiveTransactions" value="" readonly>
                </div>
            </div>
            <div class="form-group">
                <div class="col-md-8">
                    {{#block "profileExternalUser"}}

                    {{/block}}
                    <span></span>
                    <input type="submit" id="save" class="btn btn-primary" value="Save" disabled>
                </div>
            </div>
        </form>
</div>


<script type="text/javascript" src="/webjars/jquery/3.4.1/jquery.min.js"></script>
<script type="text/javascript" src="/webjars/bootstrap/4.4.1-1/js/bootstrap.min.js"></script>
<script type="text/javascript" src="/js/profileHandler.js"></script>
{{#block "scripts"}}
{{!-- Hero content goes here. --}}
{{/block}}
</body>
</html>
{{> profileExternalUser}}